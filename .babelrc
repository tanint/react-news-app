{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@/components": "./src/components",
          "@/lib": "./src/lib",
          "@/features": "./src/features",
          "@/styles": "./src/styles",
          "@/types": "./src/types",
          "@/tests": "./tests"
        }
      }
    ]
  ]
}
